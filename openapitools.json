{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.3.0",
    "generators": {
      "v1.0": {
        "generatorName": "typescript-fetch",
        "inputSpec": "#{cwd}/api/openapi/api-spec/root.json",
        "output": "#{cwd}/api/openapi/generated",
        "ignoreFileOverride": "#{cwd}/api/openapi/.openapi-generator-ignore"
      }
    }
  }
}
