{
  "name": "kottage-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "initialize": "sh scripts/submodule/init.sh & yarn",
    "test": "jest",
    "dev": "concurrently -n NEXT,TS -c magenta,cyan \"yarn dev:noTs -p 1234\" \"yarn ts --watch\"",
    "dev:noTs": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "postinstall": "patch-package & yarn generate-client",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "create-component": "node scripts/component-template/createComponent.js",
    "generate-client": "openapi-generator-cli generate --generator-key v1.0",
    "regenerate-client": "rm -r api/openapi/generated & yarn generate-client",
    "ts": "tsc --noEmit --incremental"
  },
  "dependencies": {
    "next": "11.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sharp": "0.29.3"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/preset-typescript": "7.15.0",
    "@openapitools/openapi-generator-cli": "2.4.18",
    "@storybook/addon-actions": "6.3.9",
    "@storybook/addon-essentials": "6.3.9",
    "@storybook/addon-links": "6.3.9",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addon-storyshots": "6.3.9",
    "@storybook/builder-webpack5": "6.3.9",
    "@storybook/manager-webpack5": "6.3.9",
    "@storybook/react": "6.3.9",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@types/jest": "27.0.3",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.11",
    "autoprefixer": "10.3.6",
    "babel-jest": "27.3.1",
    "babel-loader": "8.2.2",
    "babel-plugin-inline-react-svg": "2.0.1",
    "babel-preset-react-app": "10.0.0",
    "concurrently": "6.3.0",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "6.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.3.1",
    "jest-watch-typeahead": "1.0.0",
    "lint-staged": "11.2.6",
    "msw": "0.35.0",
    "node-fetch": "3.0.0",
    "patch-package": "6.4.7",
    "postcss": "8.3.11",
    "postcss-loader": "6.1.1",
    "prettier": "2.3.1",
    "react-test-renderer": "17.0.2",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "22.0.0",
    "tailwindcss": "2.2.16",
    "typescript": "4.3.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
