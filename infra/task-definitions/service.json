[
  {
    "name": "kottage_api",
    "image": "${image}",
    "essential": true,
    "portMappings": [
      {
        "containerPort": 8080
      }
    ],
    "environment": [
      {
        "name": "MYSQL_ROOT_PASSWORD",
        "value": ""
      },
      {
        "name": "MYSQL_USER",
        "value": "${mysql_user}"
      },
      {
        "name": "MYSQL_PASSWORD",
        "value": "${mysql_password}"
      },
      {
        "name": "MYSQL_DATABASE",
        "value": "${mysql_database}"
      },
      {
        "name": "MYSQL_HOST",
        "value": "${mysql_host}"
      },
      {
        "name": "DEVELOPMENT",
        "value": "false"
      },
      {
        "name": "ADMIN_NAME",
        "value": "${admin_name}"
      },
      {
        "name": "ADMIN_PASSWORD",
        "value": "${admin_password}"
      },
      {
        "name": "VERCEL_DEPOLOY_HOOK",
        "value": "${vercel_deploy_hook}"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${awslogs_group}",
        "awslogs-region": "${awslogs_region}",
        "awslogs-stream-prefix": "kottage-api"
      }
    }
  }
]
