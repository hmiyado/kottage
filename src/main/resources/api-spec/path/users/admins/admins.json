{
  "get": {
    "summary": "get admins",
    "responses": {
      "200": {
        "description": "get admins by id",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "./adminsGetResponse.json"
            }
          }
        }
      }
    }
  },
  "patch": {
    "summary": "make a user as admin",
    "requestBody": {
      "required": true,
      "content": {
        "application/json": {
          "schema": {
            "$ref": "./admin.json"
          }
        }
      }
    },
    "responses": {
      "200": {
        "description": "a user has become to admin"
      },
      "400": {
        "description": "Bad Request"
      }
    },
    "security": [
      {
        "admin": []
      }
    ]
  },
  "delete": {
    "summary": "delete a user from admin",
    "requestBody": {
      "required": true,
      "content": {
        "application/json": {
          "schema": {
            "$ref": "./admin.json"
          }
        }
      }
    },
    "responses": {
      "200": {
        "description": "a user has been deleted from admin"
      },
      "400": {
        "description": "Bad Request"
      }
    },
    "security": [
      {
        "admin": []
      }
    ]
  }
}
